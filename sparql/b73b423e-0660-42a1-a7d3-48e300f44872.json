{
        "title": "Freesurfer Model Query",
        "description": "Query for Freesurfer Stats",
        "creator": "<http://orcid.org/0000-0003-1099-3328>",
        "keyword": "select all, meta",
        "parameters": ["?subjectID", "?tag", "?vol", "?segID", "?annotation", "?max", "?mean"],
        "format": "select",
        "model": "freesurfer",
        "component": "results",
        "sparql": ["PREFIX cml: <http://www.connectomics.org/cff-2/>",
                   "PREFIX fs: <http://www.incf.org/ns/nidash/fs#>",
                   "PREFIX nidm: <http://purl.org/nidash/nidm#>",
                   "PREFIX prov: <http://www.w3.org/ns/prov#>",
                   "SELECT DISTINCT ?subjectID ?tag  ?vol ?segID  ?annotation ?max ?mean",
                   "FROM <https://gist.githubusercontent.com/nicholsn/54ca7f9e3a8210b31b7e/raw/3ac5e8a7301f3b3280a193413e158b6780a30abc/FreesurferDirStats.ttl>",
                   "WHERE {",
                   "?subjectdir a fs:SubjectDirectory ;",
                   "fs:subjectID ?subjectID ;",
                   "nidm:tag ?tag ;",
                   "prov:hadMember ?dirMember .",
                   "?statsCollection prov:wasDerivedFrom ?dirMember ;",
                   "prov:hadMember ?statsEntity .",
                   "?statsEntity a prov:Entity ;",
                   "fs:Volume_mm3 ?vol ;",
                   "fs:SegId ?segID ;",
                   "nidm:anatomicalAnnotation ?annotation ;",
                   "fs:normMax ?max ;",
                   "fs:normMean ?mean .}"],
        "reference": "",
        "version":"1.1.0",
        "@context":""

}
